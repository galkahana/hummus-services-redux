  "description": "backned for hummus services",
  "scripts": {
    "dev": "nodemon",
    "start": "node ./dist/src/main",
    "lint": "npx eslint . --fix",
    "check-tsc": "npx tsc --noEmit",
    "check": "npm run lint && npm run check-tsc",
