    "clean": "rm -rf dist",
    "dummy": "npx ts-node --files ./scripts/dummy-user.ts",
    "delete-timedout-files": "npx ts-node --files ./scripts/delete-timedout-files.ts",
    "delete-timedout-files-cron": "node ./dist/scripts/delete-timedout-files.js"
  },
