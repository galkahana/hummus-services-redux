{
  "extends": "@tsconfig/node16/tsconfig.json",
  "include": [
    "./src",
    "./config",
    "./typings",
    "./scripts",
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "./dist",
    "typeRoots": [
      "./typings"
    ],
    "baseUrl": "./src",
    "paths": {
      "@setup/*": [
        "setup/*"
      ],
      "@setup": [
        "setup"
      ],
      "@controllers/*": [
        "controllers/*"
      ],
      "@controllers": [
        "controllers"
      ],
      "@lib/*": [
        "lib/*"
      ],
      "@lib": [
        "lib"
      ],
      "@middlewares/*": [
        "middlewares/*"
      ],
      "@middlewares": [
        "middlewares"
      ],
      "@models/*": [
        "models/*"
      ],
      "@models": [
        "models"
      ],
      "@routes/*": [
        "routes/*"
      ],
      "@routes": [
        "routes"
      ],
      "@services/*": [
        "services/*"
      ],
      "@services": [
        "services"
      ]
    }
  }
}