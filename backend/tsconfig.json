{
  "extends": "@tsconfig/node16/tsconfig.json",
  "include": [
    "./src",
    "./config",
    "./typings",
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "esModuleInterop": true,
    "sourceMap": true,
    "outDir": "./dist",
    "typeRoots": [
      "./typings"
    ],
    "baseUrl": "./",
    "paths": {
      "@setup/*": [
        "./src/setup/*"
      ],
      "@setup": [
        "./src/setup"
      ],
      "@controllers/*": [
        "./src/controllers/*"
      ],
      "@controllers": [
        "./src/controllers"
      ],
      "@lib/*": [
        "./src/lib/*"
      ],
      "@lib": [
        "./src/lib"
      ],
      "@middlewares/*": [
        "./src/middlewares/*"
      ],
      "@middlewares": [
        "./src/middlewares"
      ],
      "@models/*": [
        "./src/models/*"
      ],
      "@models": [
        "./src/models"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@routes": [
        "./src/routes"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@services": [
        "./src/services"
      ]
    }
  }
}